{
  "name": "minilogue",
  "version": "1.0.0",
  "description": "Minilogue Editor.",
  "license": "MIT",
  "homepage": "https://github.com/jeffkistler/minilogue/",
  "author": "Jeff Kistler (https://github.com/jeffkistler/)",
  "scripts": {
    "start": "webpack-dev-server --hot --https --progress --colors --config webpack.dev.js",
    "build": "webpack --progress --colors -p --config webpack.prod.js",
    "lint:js": "eslint --ext .js --ext .jsx src/ webpack.*.js --cache",
    "test": "jest"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.0.1",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.0.4",
    "jest-environment-jsdom": "^22.1.4",
    "redux-devtools": "^3.4.0",
    "redux-mock-store": "^1.4.0",
    "regenerator-runtime": "^0.11.1",
    "style-loader": "^0.19.1",
    "svg-sprite-loader": "^3.6.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "2",
    "webpack-merge": "^4.1.1",
    "xmlserializer": "^0.6.1"
  },
  "dependencies": {
    "base32-decode": "^1.0.0",
    "base32-encode": "^1.0.0",
    "file-saver": "^1.3.3",
    "history": "^4.7.2",
    "jszip": "^3.1.3",
    "prop-types": "^15.5.8",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-modal": "^3.1.11",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-select": "^1.2.0",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "riek": "^1.1.0",
    "url-join": "^3.0.0"
  }
}
